<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>
<html xmlns='http://www.w3.org/1999/xhtml' xmlns:b='http://www.google.com/2005/gml/b' xmlns:data='http://www.google.com/2005/gml/data' xmlns:expr='http://www.google.com/2005/gml/expr'>
<head>
<link rel="shortcut icon" href="data:image/vnd.microsoft.icon;base64," />

<b:include data='blog' name='all-head-content'/>

<title>
      <b:if cond='data:blog.pageType == &quot;index&quot;'>
        <data:blog.pageTitle/>
        <b:else/>
        <b:if cond='data:blog.pageType != &quot;error_page&quot;'>
          <data:blog.pageName/> | <data:blog.title/>
          <b:else/>
          Page Not Found | <data:blog.title/>
        </b:if>
      </b:if>
</title>

<b:skin><![CDATA[
img                                    {border-width: 0px;}
a:link, a:visited                      {color: #39c; text-decoration: none;}
a:hover                                {text-decoration: underline}
body                                   {color: #333; background: #fff; width: 70%; margin-left: auto; margin-right: auto;
                                       font-family: 'Trebuchet MS', Trebuchet, Arial, sans-serif; font-size: 10pt; line-height: 1.4em;}
.post-body                             {padding-bottom: 30px; text-align: justify;}
.post-footer                           {font-size: 8pt; padding-bottom: 5px;}
.post-title                            {font-size: 14pt; font-weight: bold; letter-spacing: -0.06em;}
.header                                {text-align: center; margin: 25px;}
.description                           {font-size: 10pt; margin-bottom: 10px;}
.title                                 {margin: 10px 0px 10px 0px}
a.menutags                             {background: #39c; color: #fff; padding: 5px 10px 5px 10px; margin: 5px;}
.blog-feeds                            {display:none!important;} /* Hide 'subscribe to posts (atom)' */
#Attribution1                          {display:none!important;} /* Hide attribution widget */
#Navbar1                               {display:none!important;} /* Hide blogger navbar */
#Stats1                                {display:none!important;} /* Hide blogger navbar */
]]></b:skin>
</head>

<body>
TEST
<!-- HEADER -->
<div class='header'><b:section class='header-section' id='header-section' maxwidgets='1' showaddelement='no'>
     <!-- HEADER > TITLE AND DESCRIPTION -->
     <b:widget id='Header1' locked='true' title='Header' type='Header'><b:includable id='title'><a class='title' expr:href='data:blog.homepageUrl'><data:title/></a></b:includable><b:includable id='description'><div class='description'><data:description/></div></b:includable></b:widget>
     <!-- HEADER > TAGS -->
     <b:widget id='Label1' locked='false' title='Labels' type='Label'><b:includable id='main'><b:loop values='data:labels' var='label'><a class='menutags' expr:dir='data:blog.languageDirection' expr:href='data:label.url'><data:label.name/></a></b:loop></b:includable></b:widget>
</b:section></div>


<!-- CONTENT -->
<div class='content'><b:section id="content-section" class="content-section" showaddelement="yes">
     <!-- CONTENT > POSTS -->
     <b:widget id='Blog1' locked='false' mobile='yes' title='Blog' type='Blog'><b:includable id='post' var='post'>
          <!-- CONTENT > POSTS > TITLE -->
          <div class='post-title'><a expr:href='data:post.url'><data:post.title/></a></div>
          <!-- CONTENT > POSTS > FOOTER -->
          <div class='post-footer'>
               <!-- CONTENT > POSTS > FOOTER > DATE -->
               <data:post.timestamp/>
               <!-- CONTENT > POSTS > FOOTER > TAGS -->
               <b:if cond='data:post.labels'> / <b:loop values='data:post.labels' var='label'><a expr:href='data:label.url' rel='tag'><data:label.name/></a><b:if cond='data:label.isLast != &quot;true&quot;'>, </b:if></b:loop></b:if>
               <!-- CONTENT > POSTS > FOOTER > COMMENTS -->
               <b:if cond='data:post.allowComments'>/ <a class='comment-link' expr:href='data:post.addCommentUrl' expr:onclick='data:post.addCommentOnclick'>Comments: <data:post.numComments/></a></b:if>
          </div>
          <!-- CONTENT > POSTS > BODY -->
          <div class='post-body'>
               <!-- CONTENT > POSTS > BODY > ТЕХТ -->
               <data:post.body/>
               <!-- CONTENT > POSTS > BODY > JUMP LINK -->
               <b:if cond='data:post.hasJumpLink'><a expr:href='data:post.url + &quot;#more&quot;' expr:title='data:post.title'><data:post.jumpText/></a></b:if>
          </div>
     </b:includable></b:widget>
</b:section></div>

</body>
</html>